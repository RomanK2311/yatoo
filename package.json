{
  "name": "yatoo",
  "version": "0.3.0",
  "description": "Organize your life!",
  "main": "build/main.js",
  "scripts": {
    "postinstall": "patch-package",
    "build": "npm run build-src && npm run build-tw",
    "build-src": "bun run ./scripts/build.ts",
    "build-tw": "tailwindcss -i ./src/main.css -o ./build/main.css",
    "build-dev": "npm run build",
    "build-prod": "NODE_ENV=production npm run build",
    "test": "BUN_TEST=true bun test",
    "dev": "bun ./scripts/serve.ts",
    "capacitor-sync": "npx cap sync && npx cap sync @capacitor-community/electron",
    "run-android": "npx cap run android",
    "run-electron": "npx cap open @capacitor-community/electron",
    "build-android": "dotenv -- bash -c './android/gradlew -p android assembleRelease'",
    "build-electron": "cd electron && npm run electron:make",
    "release": "dotenv release-it",
    "pre-release": "dotenv npm run release -- --preRelease"
  },
  "author": {
    "name": "Paul Tirk",
    "email": "paultirk@paultirk.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/powerpaul17/yatoo.git"
  },
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@capacitor-community/electron": "^4.1.2",
    "@capacitor/android": "^5.0.5",
    "@capacitor/cli": "^5.0.5",
    "@happy-dom/global-registrator": "^11.2.0",
    "@j-ulrich/release-it-regex-bumper": "^5.0.0",
    "@release-it/bumper": "^5.1.0",
    "@release-it/keep-a-changelog": "^4.0.0",
    "@types/sinon": "^10.0.15",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/components": "^10.2.1",
    "@vueuse/core": "^10.2.1",
    "blinkdb": "^0.13.2",
    "bun": "^1.0.1",
    "bun-types": "^1.0.1",
    "daisyui": "^3.1.6",
    "dotenv-cli": "^7.2.1",
    "esbuild-plugin-vue-next": "^0.1.4",
    "eslint": "^8.42.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-vue": "^9.14.1",
    "localforage": "^1.10.0",
    "lucide-vue-next": "^0.259.0",
    "patch-package": "^7.0.0",
    "release-it": "^16.1.3",
    "sinon": "^15.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "uuid": "^8.3.2",
    "vue-eslint-parser": "^9.3.0",
    "vue-i18n": "^9.3.0-beta.26",
    "vue-router": "^4.2.2"
  }
}
